using System;
namespace CSharp_Shell
{
    public static class Program 
    {
        public static void Main() 
        {
           int i=1;
           while(i<=12)
           {
             	i++;
           if(i==5)
           {
           	continue;
           }
            Console.WriteLine(i);
           }
        }
    } 
  }
  
  output:-
2
3
4
6
