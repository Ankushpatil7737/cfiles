using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;


class Program
    {
        Program() // Non Paramiterize Constrctor
        {
            Console.WriteLine("Non Paramiterize Constrctor");
        }
        Program(string name) // Paramiterize Constrctor
        {
            Console.WriteLine("My Name is: "+name);
        }
        static void Main(string[] args)
        {
            Program p = new Program();
            Program p2 = new Program("Ankush patil");
            Console.ReadLine();
        }
    }

output:-
Non Paramiterize Constrctor
My Name is: Ankush patil

